<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".ui.auth.fragment.sign_up.MasterSignUpFragment">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical"
                android:layout_weight="1">

                <EditText
                    android:id="@+id/et_master_fullname"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginHorizontal="24dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/bg_edit_text_login"
                    android:drawablePadding="12dp"
                    android:hint="@string/fullName"
                    android:inputType="text"
                    android:maxLines="1"
                    android:imeActionLabel=""
                    android:paddingHorizontal="24sp"
                    android:paddingVertical="0dp"
                    android:textColor="@color/black1"
                    android:textCursorDrawable="@drawable/edittext_cursor_drawable"
                    android:textSize="14sp"
                    tools:ignore="TextFields" />

                <EditText
                    android:id="@+id/et_master_gender"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginHorizontal="24dp"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/bg_edit_text_login"
                    android:drawablePadding="12dp"
                    android:hint="Gender"
                    android:editable="false"
                    android:focusable="false"
                    android:inputType="number"
                    android:maxLines="1"
                    android:paddingHorizontal="24sp"
                    android:paddingVertical="0dp"
                    android:textColor="@color/black1"
                    android:textCursorDrawable="@drawable/edittext_cursor_drawable"
                    android:textSize="14sp"
                    tools:ignore="TextFields" />

                <com.vicmikhailau.maskededittext.MaskedEditText
                    android:id="@+id/et_master_birthday"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:focusable="false"
                    android:layout_marginTop="12dp"
                    android:hint="Birthday"
                    android:layout_marginHorizontal="24dp"
                    android:background="@drawable/bg_edit_text_login"
                    android:drawablePadding="12dp"
                    android:maxLength="10"
                    app:mask="##/##/####"
                    android:inputType="number"
                    android:maxLines="1"
                    android:paddingHorizontal="24sp"
                    android:paddingVertical="0dp"
                    android:textColor="@color/black1"
                    android:textCursorDrawable="@drawable/edittext_cursor_drawable"
                    android:textSize="14sp"
                    tools:ignore="TextFields" />





                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginHorizontal="24dp"
                    android:layout_marginTop="12dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">

                    <EditText
                        android:id="@+id/et_region"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginRight="12dp"
                        android:hint="Region"
                        android:paddingHorizontal="12dp"
                        android:background="@drawable/bg_edit_text_login"/>

                    <EditText
                        android:id="@+id/et_district"
                        android:layout_width="0dp"
                        android:layout_marginLeft="12dp"
                        android:hint="District"
                        android:paddingHorizontal="12dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/bg_edit_text_login"/>

<!--                    <com.chivorn.smartmaterialspinner.SmartMaterialSpinner-->
<!--                        android:id="@+id/spProfession"-->
<!--                        app:smsp_hintSize="14sp"-->
<!--                        app:smsp_enableFloatingLabel="true"-->
<!--                        android:layout_width="match_parent"-->
<!--                        android:layout_height="wrap_content"-->
<!--                        app:smsp_hint="Profession"-->
<!--                        app:smsp_isSearchable="true"-->
<!--                        app:smsp_underlineSize="0dp"-->
<!--                        app:smsp_hintColor="@color/grey"-->
<!--                        app:smsp_searchHeaderBackgroundColor="@color/black1"-->
<!--                        app:smsp_searchHeaderTextColor="@color/white"-->
<!--                        app:smsp_searchHeaderText="Profession"-->
<!--                        app:smsp_searchHint="Profession"-->
<!--                        app:smsp_itemColor="@color/black"-->
<!--                        app:smsp_itemListColor="@color/black"-->
<!--                        app:smsp_itemListHintBackgroundColor="@color/grey1"-->
<!--                        app:smsp_itemListHintColor="#FFFFFF"-->
<!--                        app:smsp_multilineError="false"-->
<!--                        app:smsp_selectedItemListColor="@color/green1" />-->


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginHorizontal="24dp"
                    android:background="@drawable/bg_edit_text_login"
                    android:layout_marginTop="12dp"
                    android:paddingHorizontal="16dp"
                    >

                    <com.chivorn.smartmaterialspinner.SmartMaterialSpinner
                        android:id="@+id/spExperience"
                        app:smsp_hintSize="14sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:smsp_hint="Experience"
                        app:smsp_isSearchable="true"
                        app:smsp_underlineSize="0dp"
                        app:smsp_hintColor="#9E9D9D"
                        app:smsp_searchHeaderBackgroundColor="@color/black1"
                        app:smsp_searchHeaderTextColor="@color/white"
                        app:smsp_searchHeaderText="Experience"
                        app:smsp_searchHint="Experience"
                        app:smsp_itemColor="@color/black"
                        app:smsp_itemListColor="@color/black"
                        app:smsp_itemListHintBackgroundColor="@color/grey1"
                        app:smsp_itemListHintColor="#FFFFFF"
                        app:smsp_multilineError="false"
                        app:smsp_selectedItemListColor="@color/green1" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginHorizontal="24dp"
                    android:background="@drawable/bg_edit_text_login"
                    android:layout_marginTop="12dp"
                    android:paddingHorizontal="16dp"
                    >

                    <com.chivorn.smartmaterialspinner.SmartMaterialSpinner
                        android:id="@+id/spLocation"
                        app:smsp_hintSize="14sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:smsp_hint="Location"
                        app:smsp_isSearchable="true"
                        app:smsp_underlineSize="0dp"
                        app:smsp_hintColor="#9E9D9D"
                        app:smsp_searchHeaderBackgroundColor="@color/black1"
                        app:smsp_searchHeaderTextColor="@color/white"
                        app:smsp_searchHeaderText="Location"
                        app:smsp_searchHint="Location"
                        app:smsp_itemColor="@color/black"
                        app:smsp_itemListColor="@color/black"
                        app:smsp_itemListHintBackgroundColor="@color/grey1"
                        app:smsp_itemListHintColor="#FFFFFF"
                        app:smsp_multilineError="false"
                        app:smsp_selectedItemListColor="@color/green1" />


                </LinearLayout>







            </LinearLayout>

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>



    <include
        android:id="@+id/progress_home"
        layout="@layout/custom_progress"
        android:visibility="gone"/>






</FrameLayout>